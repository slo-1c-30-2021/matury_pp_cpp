#include<bits/stdc++.h>
using namespace std;
int main()
{
    int licznik=0;
    int suma=0;
    string s;
    ifstream f("hasla.txt");
    while(f>>s)
        {
            for(int i=0; i<s.size(); i++)
            suma=(int)s[i]+(int)s[i+1];
            if(suma==220)
            {
                licznik++;
                cout<<s<<"\n";
                break;
            }

        }
}

